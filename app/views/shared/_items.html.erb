<%= link_to t('.users'), users_path if Current.user&.super_admin? %>
<%= link_to t('.beneficiaries'), beneficiaries_path if Current.user&.super_admin? || Current.user&.admin? %>
<%= link_to t('.deliveries'), deliveries_path if Current.user %>
<%= link_to t('.queries'), queries_path if Current.user&.super_admin? || Current.user&.admin? %>
<%= link_to t('.login'), new_session_path unless Current.user %>
<%= link_to t('.logout'), session_path(Current.user.id), data: {
  turbo_method: :delete,
  turbo_confirm: t('common.confirm')
} if Current.user %>
